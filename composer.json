{
  "name": "t-kanstantsin/yii2-action-lock-behavior",
  "description": "Allow run only one action instance at once",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "behavior"
  ],
  "homepage": "https://github.com/t-kanstantsin/yii2-action-lock-behavior",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Konstantin Timoshenko",
      "email": "t.kanstantsin@gmail.com",
      "homepage": "https://github.com/t-kanstantsin",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "^7.1",
    "psr/log": "^1.0",
    "yiisoft/yii2": "^2.0.10"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "tkanstantsin\\Yii2ActionLockBehavior\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
